{
  "name": "color-bucket",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:check": "yarn format:check && yarn lint:check && yarn types:check && yarn clean && yarn build",
    "start": "next start",
    "lint": "next lint",
    "prepare": "husky install",
    "format:check": "prettier --check .",
    "format:fix": "prettier --write .",
    "lint:check": "eslint . --ext ts --ext tsx --ext js --ext jsx",
    "lint:fix": "eslint . --fix --quiet --ignore-path .eslintignore",
    "types:check": "tsc --pretty --noEmit",
    "clean": "rimraf .next"
  },
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@mui/icons-material": "^5.11.11",
    "@mui/material": "^5.11.15",
    "@types/node": "18.15.10",
    "@types/react": "18.0.30",
    "@types/react-dom": "18.0.11",
    "eslint": "8.36.0",
    "eslint-config-next": "13.2.4",
    "lint-staged": "^13.2.1",
    "next": "13.2.4",
    "prettier": "^2.8.8",
    "react": "18.2.0",
    "react-color": "^2.19.3",
    "react-dom": "18.2.0",
    "typescript": "5.0.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.6.1",
    "@types/react": "18.0.20",
    "@types/react-color": "^3.0.6",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "eslint": "^8.23.1",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "12.3.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-node": "^0.3.6",
    "eslint-import-resolver-typescript": "^3.5.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-security": "^1.5.0",
    "husky": "^8.0.0",
    "lint-staged": "^13.0.3",
    "next-compose-plugins": "^2.2.1",
    "next-plugin-graphql": "0.0.2",
    "prettier": "^2.7.1",
    "typescript": "4.8.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "yarn format:fix",
      "yarn lint:fix"
    ],
    "*.{json,md}": [
      "yarn format:fix"
    ]
  }
}
